@using NHS111.Models.Models.Web
@model SearchJourneyViewModel
@{
    Layout = "~/Views/Shared/_LayoutNhsUK.cshtml";
}

@section Title  {NHS 111 Online - Tell us the symptom}


@section FeedbackSection {
    @{
        var feedbackDictionary = ViewContext.ViewData;
        feedbackDictionary.TemplateInfo = new TemplateInfo() {HtmlFieldPrefix = "Feedback"};
    }
    @Html.Partial("_FeedbackDetails", new FeedbackViewModel() {UserId = Model.SessionId.ToString(), PageData = Model.PageData}, feedbackDictionary)
    ;
}

@Html.Partial("_SearchBox", Model, ViewData)