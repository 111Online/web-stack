@using NHS111.Models.Models.Web
@inherits NHS111.Web.Views.Shared.SurveyLinkView<OutcomeViewModel>
@{
    Layout = "~/Views/Shared/_LayoutNhsUk.cshtml";
}

@section Title {
    NHS 111 - General information about coronavirus (COVID-19)
}

@section HeadTop {
    @{ var url = string.Format("/outcome/{0}/{1}/{2}/disposition/", Url.Encode(Model.PathwayNo), Url.Encode(Model.OutcomeGroup.Text), Url.Encode(Model.Id)); }
    <script>
        var pageId = '@Model.Id';
        dataLayer = [
        {
            'virtualPageUrl': '@url',
            'virtualPageTitle': '@Model.TitleWithoutBullets'
        }];
    </script>
}


@Html.HiddenFor(x => Model.Id)

<div class="measure nhsuk-core">

  <h1 class="nhsuk-heading-xl">
    You don’t need to speak to anybody right now as you:
  </h1>
  <ul>
    <li><b>don’t</b> have a cough</li>
    <li><b>don’t</b> have a high temperature</li>
  </ul>
  <p>It’s unlikely that you are experiencing symptoms of coronavirus (COVID-19).</p>

  <div class="callout callout--warning callout--lite nhsuk-u-margin-bottom-4">
      <p>
          You can go <a data-event-trigger="click" data-event-value="Back to homepage" href="/">back to the 111 online home page</a> and use the service to check any other symptoms you might have.
      </p>
  </div>
  <p>Or you can <a data-event-trigger="click" data-event-value="Novel Coronavirus" href="https://www.nhs.uk/conditions/coronavirus-covid-19/">read general information about coronavirus on NHS.UK</a>.</p>
</div>
