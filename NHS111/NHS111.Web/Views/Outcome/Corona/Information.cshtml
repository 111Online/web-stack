@using NHS111.Models.Models.Web
@inherits NHS111.Web.Views.Shared.SurveyLinkView<OutcomeViewModel>
@{
    Layout = "~/Views/Shared/_LayoutNhsUk.cshtml";
}

@section Title {
    NHS 111 - Looking after yourself
}

@section HeadTop {
    @{ var url = string.Format("/outcome/{0}/{1}/{2}/disposition/", Url.Encode(Model.PathwayNo), Url.Encode(Model.OutcomeGroup.Text), Url.Encode(Model.Id)); }
    <script>
        var pageId = '@Model.Id';
        dataLayer = [
        {
            'virtualPageUrl': '@url',
            'virtualPageTitle': '@Model.TitleWithoutBullets'
        }];
    </script>
}
@section SurveyBanner{
    @Html.Partial("_SurveyLink", Model.SurveyLink)
}
@section FeedbackSection {
    @if (!SurveyLinkFeature.IsEnabled)
    {
        @Html.Partial("_FeedbackDetails", new FeedbackViewModel() { UserId = Model.SessionId.ToString(), PageData = new PageDataViewModel(PageDataViewModel.PageType.Outcome, Model) })
    }
    else
    {
        @Html.Partial("_SurveyLink", Model.SurveyLink)
    }
}

@Html.HiddenFor(x => Model.Id)
<h1 class="nhsuk-heading-l">
    Read general information about coronavirus (COVID19)
</h1>

<p>As you:</p>
<ul>
    <li>have <strong>not</strong> travelled to a country with a serious outbreak</li>
    <li>are <strong>not</strong> showing any symptoms</li>
    <li>have <strong>not</strong> been in contact with a confirmed case</li>
</ul>

<p>You don’t need to speak to anybody right now.</p>

<p>
    You can <a href="javascript: return false;">read advice and find answers to common questions about coronavirus (COVID19) on NHS.UK</a>
</p>