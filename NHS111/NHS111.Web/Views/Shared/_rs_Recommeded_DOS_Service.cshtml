@model List<NHS111.Models.Models.Web.ServiceViewModel>

    <div class="measure">
        <h1>Where to get help</h1>

    </div>
@if (Model.Count > 0)
{
    var partial = "";
    if (Model.First().ServiceType.Id == 25) { partial = "_rs_service_callback";}
    if (Model.First().ServiceType.Id == 13) { partial = "_rs_PhysicalLocationAppointmentType"; }
    <div>
        @Html.Partial(partial, Model.First())
    </div>
}