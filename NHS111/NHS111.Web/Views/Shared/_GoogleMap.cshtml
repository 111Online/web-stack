@model NHS111.Models.Models.Web.OutcomeMapViewModel
@{
    Layout = null;
    var apiUrl = "https://maps.googleapis.com/maps/api/js?key=" + Model.MapsApiKey;
}
<style>

    /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
    .service-map {
        height: 100%;
    }
    /* Optional: Makes the sample page fill the window. */
    html, body {
        height: 100%;
        margin: 0;
        padding: 0;
    }
</style>

<div class="service-map" role="presentation"></div>
<script src="@apiUrl" type="text/javascript"></script>
<script src="@NHS111.Web.Helpers.Versioning.GetVersionedUriRef("~/Content/js/bundle-map.js")" type="text/javascript"></script>