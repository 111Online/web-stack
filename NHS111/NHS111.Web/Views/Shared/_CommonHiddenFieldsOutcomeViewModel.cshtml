@model NHS111.Models.Models.Web.OutcomeViewModel

@Html.HiddenFor(x => Model.Id)
@Html.HiddenFor(x => Model.JourneyJson)
@Html.HiddenFor(x => Model.PathwayId)
@Html.HiddenFor(x => Model.PathwayNo)
@Html.HiddenFor(x => Model.PathwayTitle)
@Html.HiddenFor(x => Model.SymptomGroup)
@Html.HiddenFor(x => Model.UserInfo.Age)
@Html.HiddenFor(x => Model.UserInfo.Gender)
@Html.HiddenFor(x => Model.SymptomDiscriminator)
@Html.HiddenFor(x => Model.Title)
@Html.HiddenFor(x => Model.UserInfo.CurrentAddress.PostCode)
@Html.HiddenFor(x => Model.UserId)
@Html.HiddenFor(x => Model.StateJson)
@Html.HiddenFor(x => Model.PreviousStateJson)
@if (Model.CheckCapacitySummaryResultList != null)
{
    for (var i = 0; i < Model.CheckCapacitySummaryResultList.Count(); ++i)
    {
        @Html.HiddenFor(m => m.CheckCapacitySummaryResultList[i].IdField)
        @Html.Hidden(string.Format("CheckCapacitySummaryResultList[{0}].NameField", i), @WebUtility.HtmlDecode(Model.CheckCapacitySummaryResultList[i].NameField))
        @Html.Hidden(string.Format("CheckCapacitySummaryResultList[{0}].AddressField", i), @WebUtility.HtmlDecode(Model.CheckCapacitySummaryResultList[i].AddressField))
        @Html.HiddenFor(m => m.CheckCapacitySummaryResultList[i].OdsCodeField)
        @Html.HiddenFor(m => m.CheckCapacitySummaryResultList[i].PostcodeField)
    }
}
@Html.HiddenFor(m => m.SurgeryViewModel.SelectedSurgery)

@for (int i = 0; i < Model.CollectedKeywords.Count; i++)
{
    @Html.HiddenFor(model => Model.CollectedKeywords[i])
}

